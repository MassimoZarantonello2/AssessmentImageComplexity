import re

def parse(path):
    with open(path, 'r') as f:
        with open("./IC9600/splitted_train.txt", 'w') as f2:
            lines = f.readlines()
            for line in lines:
                category,x = line.split("_", 1)
                name,cpl_score = x.split("  ", 1)
                f2.write(category + " " + name + " " + cpl_score)
            


            
if __name__ == "__main__":
    path = "./IC9600/train.txt"
    parse(path)